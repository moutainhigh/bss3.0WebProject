<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asia.mapper.ordmapper.ProdInstRouteMapper">
  <sql id="Base_Column_List">
    PROD_INST_ID, ROUTE_ID
  </sql>
  <select id="selectProdInstRouteId" parameterType="java.lang.Long" resultType="java.util.HashMap">
    select
    <include refid="Base_Column_List" />
