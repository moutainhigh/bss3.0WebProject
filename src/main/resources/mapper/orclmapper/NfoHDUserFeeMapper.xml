<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asia.mapper.orclmapper.NfoHDUserFeeMapper">
  <select id="queryTotalConsumed"  resultType="java.lang.String">
   SELECT a.bill_total_fee
   FROM INFO_HD_USER_FEE  a
   WHERE a.prod_id =#{servId}
  </select>
</mapper>