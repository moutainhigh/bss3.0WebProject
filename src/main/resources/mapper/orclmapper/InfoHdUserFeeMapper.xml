  <sql id="Base_Column_List">
    OFFER_ID, PROD_ID, OFFER_TYPE, OFFER_START_DATE, BILL_TOTAL_FEE, CRM_TOTAL_FEE, END_TYPE, 
    RENT_MONTH, OFFER_SPEC_ID
  </sql>
  <select id="selectInfoHdUserFee" parameterType="java.util.HashMap" resultMap="BaseResultMap">
