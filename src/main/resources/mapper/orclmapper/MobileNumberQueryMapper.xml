<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asia.mapper.orclmapper.MobileNumberQueryMapper">
  <select id="moBileNumberQuery"  resultType="java.lang.String">
     select  count(1) cnt  from INTERFACE.V_MOBILE_HEAD a
     WHERE a.HEAD = SUBSTR(#{accNbr}, 1, 7)
  </select>
</mapper>