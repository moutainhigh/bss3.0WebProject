<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asia.mapper.orclmapper.CustomerMapper">
  <select id="queryCustNameByCustId"  resultType="java.lang.String">
     select a.cust_name
     from usr_profile.customer@link_to_acct a
     where a.cust_id=#{custId}
  </select>
</mapper>